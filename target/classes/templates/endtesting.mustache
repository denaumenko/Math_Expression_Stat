            <tr>
                <th scope="row">{{# Questions }}
                    {{ . }}
                {{/ Questions }}</th>
                <td>{{# TotalNumberCorect }}
                    {{ . }}
                {{/ TotalNumberCorect }}</td>

                <td>{{# TotalPercent }}
                    {{ . }}
                {{/ TotalPercent }}</td>

            </tr>
            </tbody>
        </table>

        <table class="table table-bordered">
            <thead>
            <tr>

                <th scope="col">Max answer time</th>
                <th scope="col">Min answer time</th>
                <th scope="col">Avg answer time</th>
                <th scope="col">Number of enters</th>

            </tr>
            </thead>
            <tbody>
            <tr>
                <td>{{# TotalMaxAllTime }}
                    {{ . }}
                {{/ TotalMaxAllTime }}</td>
                <td>{{# TotalMinAllTime }}
                    {{ . }}
                {{/ TotalMinAllTime }}</td>
                <td>{{# TotalAvgAllTime }}
                    {{ . }}
                {{/ TotalAvgAllTime }}</td>
                <td>{{# TotalEnters }}
                    {{ . }}
                {{/ TotalEnters }}</td>

            </tr>
            </tbody>
        </table>
        </div>
    </div>
  </div>
</div>

